        android:textSize="50sp"
        android:layout_marginBottom="10dp" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="323dp"
            android:layout_weight="0.92"
            android:layout_marginEnd="20dp"
            android:layout_height="40dp"
            android:textSize="25sp"
            android:layout_marginStart="10dp"
            android:textColor="@color/colorBlack"
            android:text="@string/scaleAbility"
            android:id="@+id/abilityToScaleBenchText" />

        <ToggleButton
            android:text="@string/toggleButton"
            android:layout_height="72dp"
            android:id="@+id/abilityScaleBenchButton"
            android:layout_width="130dp"
            android:textOff="@string/no"
            android:textOn="@string/yes"
            android:backgroundTint="@color/colorSparxRed"
            android:textColor="@color/colorWhite"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/placesCanScaleFromLinear">

        <TextView
            android:layout_width="323dp"
            android:layout_weight="0.92"
            android:layout_marginEnd="20dp"
            android:layout_height="40dp"
            android:textSize="25sp"
            android:layout_marginStart="10dp"
            android:textColor="@color/colorBlack"
            android:text="@string/scalePlaces"
            android:id="@+id/placesCanScaleBenchText" />

        <EditText
            android:layout_width="130dp"
            android:layout_height="72dp"
            android:ems="10"
            android:id="@+id/placesCanScaleBenchInput"
            android:layout_weight="0.92"
            android:textColor="@color/colorBlack"
            android:textAlignment="center"
            android:textSize="15sp"
            android:inputType="textMultiLine" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/prefPlaceToScaleLinear">

        <TextView
            android:layout_width="323dp"
            android:layout_weight="0.92"
            android:layout_marginEnd="20dp"
            android:layout_height="40dp"
            android:textSize="25sp"
            android:layout_marginStart="10dp"
            android:textColor="@color/colorBlack"
            android:text="@string/scalePreferredPlace"
            android:id="@+id/preferredPlacesScaleBenchText" />

        <EditText
            android:layout_width="130dp"
            android:layout_height="72dp"
            android:ems="10"
            android:id="@+id/preferredPlacesScaleInput"
            android:layout_weight="0.92"
            android:textColor="@color/colorBlack"
            android:textAlignment="center"
            android:textSize="30sp"
            android:inputType="textMultiLine" />
    </LinearLayout>
<!-- Begin Other Section -->

    <TextView
        android:text="@string/other"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/otherHeadlineBench"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        android:textSize="50sp"
        android:layout_marginBottom="10dp" />


    <EditText
        android:layout_width="match_parent"
        android:ems="10"
        android:id="@+id/autoAbilitiesBench"
        android:hint="@string/autoAbilities"
        android:textSize="18sp"
        android:layout_margin="5dp"
        android:layout_marginBottom="0dp"
        android:layout_weight="0.14"
        android:layout_height="80dp"
        android:inputType="textMultiLine" />

    <EditText
        android:layout_width="match_parent"
        android:ems="10"
        android:id="@+id/commentsBench"
        android:hint="@string/otherComments"
        android:textSize="25sp"
        android:layout_margin="5dp"
        android:layout_marginBottom="0dp"
        android:layout_weight="0.14"
        android:layout_height="80dp"
        android:inputType="textMultiLine" />



