        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="323dp"
            android:layout_weight="0.92"
            android:layout_marginRight="20dp"
            android:layout_height="40dp"
            android:textSize="25dp"
            android:layout_marginLeft="10dp"
            android:textColor="@color/colorBlack"
            android:text="Ability To Scale"
            android:id="@+id/abilityToScaleBenchText" />

        <ToggleButton
            android:text="ToggleButton"
            android:layout_height="72dp"
            android:id="@+id/abilityScaleBenchButton"
            android:layout_width="130dp"
            android:textOff="No"
            android:textOn="Yes"
            android:backgroundTint="@color/colorSparxRed"
            android:textColor="@color/colorWhite"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="323dp"
            android:layout_weight="0.92"
            android:layout_marginRight="20dp"
            android:layout_height="40dp"
            android:textSize="25dp"
            android:layout_marginLeft="10dp"
            android:textColor="@color/colorBlack"
            android:text="Places They Can Scale From"
            android:id="@+id/placesCanScaleBenchText" />

        <EditText
            android:layout_width="130dp"
            android:layout_height="72dp"
            android:ems="10"
            android:id="@+id/placesCanScaleBenchInput"
            android:layout_weight="0.92"
            style="@android:style/TextAppearance.Material"
            android:textColor="@color/colorBlack"
            android:textAlignment="center"
            android:textSize="15dp"
            android:inputType="textMultiLine" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="323dp"
            android:layout_weight="0.92"
            android:layout_marginRight="20dp"
            android:layout_height="40dp"
            android:textSize="25dp"
            android:layout_marginLeft="10dp"
            android:textColor="@color/colorBlack"
            android:text="Preferred Scale Place"
            android:id="@+id/preferredPlacesScaleBenchText" />

        <EditText
            android:layout_width="130dp"
            android:layout_height="72dp"
            android:ems="10"
            android:id="@+id/preferredPlacesScaleInput"
            android:layout_weight="0.92"
            style="@android:style/TextAppearance.Material"
            android:textColor="@color/colorBlack"
            android:textAlignment="center"
            android:textSize="30dp"
            android:inputType="textMultiLine" />
    </LinearLayout>
<!-- Begin Other Section -->

    <TextView
        android:text="Other"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/otherHeadlineBench"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        android:textSize="50dp"
        android:layout_marginBottom="10dp" />


    <EditText
        android:layout_width="match_parent"
        android:ems="10"
        android:id="@+id/autoAbilitiesBench"
        android:hint="Auto Abilities"
        android:textSize="18dp"
        android:layout_margin="5dp"
        android:layout_marginBottom="0dp"
        android:layout_weight="0.14"
        android:layout_height="80dp"
        android:inputType="textMultiLine" />

    <EditText
        android:layout_width="match_parent"
        android:ems="10"
        android:id="@+id/commentsBench"
        android:hint="Other comments"
        android:textSize="25px"
        android:layout_margin="5dp"
        android:layout_marginBottom="0dp"
        android:layout_weight="0.14"
        android:layout_height="80dp"
        android:inputType="textMultiLine" />



